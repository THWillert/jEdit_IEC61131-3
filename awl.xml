<?xml version="1.0"?>
<!DOCTYPE MODE SYSTEM "xmode.dtd">
<!--IEC61131 mode V0.1 by Thorsten Willert (thorsten.willert [ at ] gmx.de)
!!! IL ist deprecated!
Can be used for Structured Text, too.

Catalog entry:
================================================================================

<MODE NAME="AWL"			FILE="awl.xml"
						FILE_NAME_GLOB="*.{awl}"/>

================================================================================

-->
<MODE>
	<PROPS>
		<PROPERTY NAME="lineComment" VALUE="//" />
		<PROPERTY NAME="folding" VALUE="indent" />
		<PROPERTY NAME="wordBreakChars" VALUE=",+-=&lt;&gt;/?^&amp;*" />
		<PROPERTY NAME="noWordSep" VALUE="_" />
	</PROPS>

	<RULES IGNORE_CASE="TRUE"
	HIGHLIGHT_DIGITS="TRUE"
	DIGIT_RE="(0[lL]?|[1-9]\d{0,9}(\d{0,9}[lL])?|0[xX]\p{XDigit}{1,8}(\p{XDigit}{0,8}[lL])?|0[0-7]{1,11}([0-7]{0,11}[lL])?|([0-9]+\.[0-9]*|\.[0-9]+)([eE][+-]?[0-9]+)?[fFdD]?|[0-9]+([eE][+-]?[0-9]+[fFdD]?|([eE][+-]?[0-9]+)?[fFdD]))">

<!-- ###  Extensions  ########################################################## -->


<!-- ########################################################################### -->

		<EOL_SPAN TYPE="COMMENT1">//</EOL_SPAN>

		<EOL_SPAN TYPE="MARKUP">TITLE</EOL_SPAN>

		<!-- "Strings" -->
		<SPAN TYPE="LITERAL1" NO_LINE_BREAK="TRUE">
			<BEGIN>'</BEGIN>
			<END>'</END>
		</SPAN>
		<!-- "Symbolik" -->
		<SPAN TYPE="LITERAL2" NO_LINE_BREAK="TRUE">
			<BEGIN>"</BEGIN>
			<END>"</END>
		</SPAN>
		
		<!-- Comments -->
		<SPAN TYPE="COMMENT1" NO_LINE_BREAK="TRUE">
			<BEGIN>(*</BEGIN>
			<END>*)</END>
		</SPAN>


		<MARK_PREVIOUS TYPE="LABEL" AT_WHITESPACE_END="TRUE" EXCLUDE_MATCH="TRUE">:</MARK_PREVIOUS>

		<SEQ TYPE="OPERATOR">=</SEQ>
		<SEQ TYPE="OPERATOR">[</SEQ>
		<SEQ TYPE="OPERATOR">]</SEQ>
		<SEQ TYPE="OPERATOR">(</SEQ>
		<SEQ TYPE="OPERATOR">)</SEQ>
		<SEQ TYPE="OPERATOR">:=</SEQ>

		<!-- Data -->
		<SEQ_REGEXP TYPE="LITERAL4">2#(1|0)+</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="LITERAL4">8#.*?\b</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="LITERAL4">(TIME|DATE|DAY_AND_TIME|DT|TOD|TIME_OF_DAY)#.*?\b</SEQ_REGEXP>

		<SEQ_REGEXP TYPE="LITERAL4">(?i)(true|false)</SEQ_REGEXP>

		<SEQ_REGEXP TYPE="INVALID">\w+#\w+</SEQ_REGEXP>

		<!-- Address -->
		<SEQ_REGEXP TYPE="LITERAL3">\b%(IB|IW|ID|IX|QB|QW|QD|MB|MW|MD|ML|MX\d+\.)\d+</SEQ_REGEXP>

		<!-- -->
		<SEQ_REGEXP TYPE="LITERAL3">\b(FB|FC)\s*\d+</SEQ_REGEXP>

		<!-- declaration -->
		<SEQ_REGEXP TYPE="LITERAL3">\s+:=</SEQ_REGEXP>
		<SEQ_REGEXP TYPE="LITERAL3">\s+=></SEQ_REGEXP>

		<KEYWORDS>
		<!-- ST -->
			<KEYWORD1>IF</KEYWORD1>
			<KEYWORD1>THEN</KEYWORD1>
			<KEYWORD1>ELSIF</KEYWORD1>
			<KEYWORD1>ELSE</KEYWORD1>
			<KEYWORD1>END_IF</KEYWORD1>
			
			<KEYWORD1>CASE</KEYWORD1>
			<KEYWORD1>OF</KEYWORD1>
			<KEYWORD1>ELSE</KEYWORD1>
			<KEYWORD1>END_CASE</KEYWORD1>
			
			<KEYWORD1>FOR</KEYWORD1>
			<KEYWORD1>TO</KEYWORD1>
			<KEYWORD1>DO</KEYWORD1>
			<KEYWORD1>END_FOR</KEYWORD1>
			
			<KEYWORD1>WHILE</KEYWORD1>
			<KEYWORD1>END_WHILE</KEYWORD1>
			
			<KEYWORD1>REPEAT</KEYWORD1>
			<KEYWORD1>UNTIL</KEYWORD1>
			<KEYWORD1>END_REPEAT</KEYWORD1>
			
			<KEYWORD1>EXIT</KEYWORD1>
			<KEYWORD1>RETURN</KEYWORD1>
			<KEYWORD1>NIL</KEYWORD1>
			
			<KEYWORD1>CAL</KEYWORD1>
		<!-- Standard functions -->
			<!-- Numerical -->
			
			<KEYWORD1>ABS</KEYWORD1>
			<KEYWORD1>SQRT</KEYWORD1>
			<KEYWORD1>LN</KEYWORD1>
			<KEYWORD1>LOG</KEYWORD1>
			<KEYWORD1>EXP</KEYWORD1>
			<KEYWORD1>SIN</KEYWORD1>
			<KEYWORD1>COS</KEYWORD1>
			<KEYWORD1>TAN</KEYWORD1>
			<KEYWORD1>ASIN</KEYWORD1>
			<KEYWORD1>ACOS</KEYWORD1>
			<KEYWORD1>ATON</KEYWORD1>
			
			<!-- Arithmetic -->
			<KEYWORD1>ADD</KEYWORD1>
			<KEYWORD1>MUL</KEYWORD1>
			<KEYWORD1>SUB</KEYWORD1>
			<KEYWORD1>ABS</KEYWORD1>
			<KEYWORD1>DIV</KEYWORD1>
			<KEYWORD1>MOD</KEYWORD1>
			<KEYWORD1>EXPT</KEYWORD1>
			<KEYWORD1>MOVE</KEYWORD1>
			
			<!-- Bit-shift -->
			<KEYWORD1>SHL</KEYWORD1>
			<KEYWORD1>SHR</KEYWORD1>
			<KEYWORD1>ROR</KEYWORD1>
			<KEYWORD1>ROL</KEYWORD1>
			
			<!-- Bitwise -->
			<KEYWORD1>LD</KEYWORD1>
			<KEYWORD1>ST</KEYWORD1>
			
			<KEYWORD1>AND</KEYWORD1>
			<KEYWORD1>ANDN</KEYWORD1>
			<KEYWORD1>OR</KEYWORD1>
			<KEYWORD1>ORN</KEYWORD1>
			<KEYWORD1>XOR</KEYWORD1>
			<KEYWORD1>NOT</KEYWORD1>
			
			<!-- Selection -->
			<KEYWORD1>SEL</KEYWORD1>
			<KEYWORD1>MAX</KEYWORD1>
			<KEYWORD1>MIN</KEYWORD1>
			<KEYWORD1>LIMIT</KEYWORD1>
			<KEYWORD1>MUX</KEYWORD1>
			
			<!-- Comparions -->
			<KEYWORD1>GT</KEYWORD1>
			<KEYWORD1>GE</KEYWORD1>
			<KEYWORD1>EQ</KEYWORD1>
			<KEYWORD1>LT</KEYWORD1>
			<KEYWORD1>LE</KEYWORD1>
			<KEYWORD1>NE</KEYWORD1>
			
			<!-- Character string -->
			<KEYWORD1>LEN</KEYWORD1>
			<KEYWORD1>LEFT</KEYWORD1>
			<KEYWORD1>RIGHT</KEYWORD1>
			<KEYWORD1>MID</KEYWORD1>
			<KEYWORD1>CONCAT</KEYWORD1>
			<KEYWORD1>INSERT</KEYWORD1>
			<KEYWORD1>DELETE</KEYWORD1>
			<KEYWORD1>REPLACE</KEYWORD1>
			<KEYWORD1>FIND</KEYWORD1>
			
			
		<!-- std. FB -->
			<!-- bistable -->
			<KEYWORD1>SR</KEYWORD1>
			<KEYWORD1>RS</KEYWORD1>
			
			<!-- edge detection -->
			<KEYWORD1>R_TRIG</KEYWORD1>
			<KEYWORD1>F_TRIG</KEYWORD1>
			
			<!-- counters -->
			<KEYWORD1>CTU</KEYWORD1>
			<KEYWORD1>CTD</KEYWORD1>
			<KEYWORD1>CTUD</KEYWORD1>
			
			<!-- timers -->
			<KEYWORD1>TP</KEYWORD1>
			<KEYWORD1>TON</KEYWORD1>
			<KEYWORD1>TOF</KEYWORD1>
			<KEYWORD1>RTC</KEYWORD1>
			

			<!--<KEYWORD4>T</KEYWORD4>-->
			<KEYWORD4>Z</KEYWORD4>

			<KEYWORD4>UO</KEYWORD4>
			<KEYWORD4>OS</KEYWORD4>
			<KEYWORD4>BIE</KEYWORD4>
			<KEYWORD4>OV</KEYWORD4>

			<!-- Datentypen -->
			<KEYWORD3>BOOL</KEYWORD3>
			<KEYWORD3>BYTE</KEYWORD3>
			<KEYWORD3>WORD</KEYWORD3>
			<KEYWORD3>DWORD</KEYWORD3>
			<KEYWORD3>LWORD</KEYWORD3>
			
			<KEYWORD3>SINT</KEYWORD3>
			<KEYWORD3>INT</KEYWORD3>
			<KEYWORD3>DINT</KEYWORD3>
			<KEYWORD3>LINT</KEYWORD3>
			
			<KEYWORD3>USINT</KEYWORD3>
			<KEYWORD3>UINT</KEYWORD3>
			<KEYWORD3>UDINT</KEYWORD3>
			<KEYWORD3>ULINT</KEYWORD3>
			
			<KEYWORD3>REAL</KEYWORD3>
			<KEYWORD3>LREAL</KEYWORD3>
			
			<KEYWORD3>DATE</KEYWORD3>
			<KEYWORD3>TOD</KEYWORD3>
			<KEYWORD3>TIME_OF_DAY</KEYWORD3>
			<KEYWORD3>DT</KEYWORD3>
			<KEYWORD3>DATE_AND_TIME</KEYWORD3>
			
			<KEYWORD3>TIME</KEYWORD3>
			<KEYWORD3>STRING</KEYWORD3>
			
			<KEYWORD3>ARRAY</KEYWORD3>
			<KEYWORD3>POINTER</KEYWORD3>


			<!-- Parametertypen -->
			<KEYWORD3>ANY</KEYWORD3>

			<!-- AWL-Quelle -->
				<!-- Configuration -->
				<MARKUP>CONFIGURATION</MARKUP>
				<MARKUP>VAR_GLOBAL</MARKUP>
				<MARKUP>VAR_CONFIG</MARKUP>
				<MARKUP>RESOURCE</MARKUP>
				<MARKUP>TASK</MARKUP>
				<MARKUP>PROGRAM</MARKUP>
				<MARKUP>WITH</MARKUP>
				<MARKUP>END_RESOURCE</MARKUP>
				<MARKUP>END_CONFIGURATION</MARKUP>
			
				<MARKUP>PROGRAM</MARKUP>
				<MARKUP>END_PROGRAM</MARKUP>

				<MARKUP>FUNCTION_BLOCK</MARKUP>
				<MARKUP>END_FUNCTION_BLOCK</MARKUP>

				<MARKUP>FUNCTION</MARKUP>
				<MARKUP>END_FUNCTION</MARKUP>

				<MARKUP>DATA_BLOCK</MARKUP>
				<MARKUP>END_DATA_BLOCK</MARKUP>

				<!-- Deklaration -->
				<MARKUP>VAR</MARKUP>
				<MARKUP>VAR_INPUT</MARKUP>
				<MARKUP>VAR_OUTPUT</MARKUP>
				<MARKUP>VAR_IN_OUT</MARKUP>
				<MARKUP>VAR_EXTERNAL</MARKUP>
				<MARKUP>VAR_GLOBAL</MARKUP>
				<MARKUP>VAR_ACCESS</MARKUP>
				<MARKUP>END_VAR</MARKUP>
				
				<MARKUP>RETAIN</MARKUP>
				<MARKUP>CONSTANT</MARKUP>
				<MARKUP>AT</MARKUP>

				<MARKUP>EN</MARKUP>
				<MARKUP>ENO</MARKUP>

				<!-- Code -->
				<MARKUP>BEGIN</MARKUP>
				<MARKUP>NETWORK</MARKUP>

				<!-- Beschreibung -->
				<MARKUP>TITLE</MARKUP>
				<MARKUP>KNOW_HOW_PROTECTED</MARKUP>
				<MARKUP>AUTHOR</MARKUP>
				<MARKUP>FAMILY</MARKUP>
				<MARKUP>NAME</MARKUP>
				<MARKUP>VERSION</MARKUP>

				<!-- FB -->
				<MARKUP>CODE_VERSION1</MARKUP>

				<!-- DB -->
				<MARKUP>UNLINKED</MARKUP>
				<MARKUP>READ_ONLY</MARKUP>

				<!-- Struct -->
				<MARKUP>TYPE</MARKUP>
				<MARKUP>STRUCT</MARKUP>
				<MARKUP>END_STRUCT</MARKUP>
				<MARKUP>END_TYPE</MARKUP>
		</KEYWORDS>
	</RULES>

</MODE>
